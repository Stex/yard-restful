<% if object.restful? && !object.resource? %>
  <h3 class="object_properties_title">Object properties</h3>
  <%= yieldall(item: :attr, title: 'Non-System Attributes') %>
  <%= yieldall(item: :association, title: 'Associations') %>
  <% if object.tags(:example).size > 0 %>
    <h3>Examples</h3>
    <% object.tags(:example).each do |tag| %>
      <div class="example">
        <%= htmlify(tag.text) %>
      </div>
    <% end %>
  <% end %>
<% end %>
